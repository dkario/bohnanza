    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "latest",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "pinst": "^2.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc --noEmit",
      "eslint --cache --fix"
    ]
