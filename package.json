{
  "name": "bohn",
  "packageManager": "yarn@3.1.1",
  "version": "0.0.0",
  "description": "bohner",
  "main": "index.ts",
  "author": "dyl",
  "license": "MIT",
  "scripts": {
    "start": "babel src --extensions .ts -d dist",
